        <script src="https://cdnjs.cloudflare.com/ajax/libs/PapaParse/5.1.0/papaparse.min.js"></script>
	
        <script type="text/javascript"charset="utf-8">

        // initialization and configuration   
        $(document).ready( function () {
           var bicitable = $('#databicitable').dataTable( {
               "fixedHeader": true,
	       "bPaginate": false,
	       "bInfo": false, // hide showing entries
               "lengthMenu": [ 1000, 100, 75, 50, 25, 10 ],
               "columns": [
		   { "width": "16px" },
                   { "width": "50px" },
                   null,
                   { "width": "16px", "orderable": false },
               ]
              //"info":     false, // remove header and footer
              //"ordering": false,
	      //"bPaginate": false,
	      //"sDom": 'T<"clear">lfrtip'
	   } ); // end dataTable                                                                               
           Papa.parse("ciclozingarate/bici.csv", {
               //header: true,
               download: true,
               complete: function(results) {
	           results.data.forEach(function (x) {
		       if (x.length==5) {

			   if (x[4].length > 0) {
			       switch (x[1]) {
			       case "INFO":
				   x[1] = "<div style='display: none'>INFO</div><img src='img/info.svg' alt='info' height='24' width='24'>";
				   break;
			       case "AGENZIA":
				   x[1] = "<div style='display: none'>AGENZIA</div><img src='img/agenzia.svg' alt='agenzia' height='24' width='24'>";
				   break;
			       default:
				   x[1] = "<div style='display: none'>ZZZ</div>";
			       }
			       x[4] = "<a href='" + x[4] +
				   "' target='_blank'><img src='img/WWW_logo_by_Robert_Cailliau.svg' alt='Link' height='32' width='32'></a>";
			   }
                           x.shift(); // remove first element
			   bicitable.fnAddData(x)
		       } else {
			   console.log("Skipping ");
			   console.log(x);
		       }
		   });
               }
           });
	
        } ); // end ready function
        // END initialization and configuration
	  
        </script>

